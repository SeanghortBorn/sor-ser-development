{
    "name": "sor-ser",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "build:analyze": "vite build --mode analyze",
        "build:stats": "vite build --mode production && node -e \"console.log('Build complete! Check public/build/manifest.json for chunk details.')\"",
        "dev": "vite",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "bump-semver": "node .github/scripts/bump_semver.js",
        "gen-changelog": "node .github/scripts/generate_changelog.js",
        "release:preview": "node .github/scripts/analyze_commits.js && cat .release_info.json",
        "release:notes": "node .github/scripts/analyze_commits.js && node .github/scripts/bump_version_auto.js && node .github/scripts/generate_release_notes.js && cat RELEASE_NOTES.md",
        "commit:help": "bash .github/scripts/commit-help.sh"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/postcss": "^4.1.17",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@vitejs/plugin-react": "^5.1.2",
        "autoprefixer": "^10.4.22",
        "jsdom": "^27.3.0",
        "laravel-vite-plugin": "^2.0.1",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.17",
        "terser": "^5.44.1",
        "vite": "^7.0.0",
        "vitest": "^4.0.15"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^7.1.0",
        "@headlessui/react": "^2.2.9",
        "@inertiajs/react": "^2.2.20",
        "@popperjs/core": "^2.11.8",
        "admin-lte": "3.2",
        "axios": "^1.13.2",
        "bootstrap": "^5.3.3",
        "exceljs": "^4.4.0",
        "flag-icons": "^7.5.0",
        "framer-motion": "^12.23.25",
        "jquery": "^3.7.1",
        "lucide-react": "^0.556.0",
        "moment": "^2.30.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-is": "^18.3.1",
        "recharts": "^3.5.1",
        "zustand": "^5.0.9"
    }
}
